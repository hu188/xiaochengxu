<view class="cart">
  <view>
    <view class="icon">
      <badge count='{{count}}'>
        <icon type="publishgoods_fill" color="#fff" size="32"></icon>
      </badge>
    </view>
    <text class="tips">{{count*1 === 0 ? '未选购商品' : "总计￥"+price}}</text>
  </view>
  <view  onTap="onSubmit">去结算</view>
</view>